<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="dmdd.dmddjava.dataaccess.dataobject.PeriodRollTask" 
    table="PERIODROLLTASK"
>

    <id
        name="id"
        type="java.lang.Long"
        column="ID"
        unsaved-value="0"
    >
        <meta attribute="use-in-equals">true</meta>
        <generator class="identity" />
    </id>

    <version
        name="version"
        type="long"
        column="VERSION"
    />

    <property
        name="compilePeriod"
        type="int"
        column="COMPILEPERIOD"
        not-null="true"
        length="6"
    />
    <property
        name="seqNo"
        type="int"
        column="SEQNO"
        not-null="true"
        length="6"
    />
    <property
        name="category"
        type="int"
        column="CATEGORY"
        not-null="true"
        length="2"
    />
    <property
        name="status"
        type="int"
        column="STATUS"
        not-null="true"
        length="2"
    />
    <property
        name="createTime"
        type="java.sql.Timestamp"
        column="CREATETIME"
        not-null="true"
        length="10"
    />
    <property
        name="beginTime"
        type="java.sql.Timestamp"
        column="BEGINTIME"
        length="10"
    />
    <property
        name="endTime"
        type="java.sql.Timestamp"
        column="ENDTIME"
        length="10"
    />
    <property
        name="comments"
        type="java.lang.String"
        column="COMMENTS"
        length="1024"
    />

    <!-- Associations -->
  
    <!-- uni-directional many-to-one association to Organization -->
    <many-to-one
        name="organization"
        class="dmdd.dmddjava.dataaccess.dataobject.Organization"
    >
        <column name="ORGANIZATIONID" />
    </many-to-one>
    <!-- uni-directional many-to-one association to ForecastInst -->
    <many-to-one
        name="forecastInst"
        class="dmdd.dmddjava.dataaccess.dataobject.ForecastInst"
    >
        <column name="FORECASTINSTID" />
    </many-to-one>

</class>
</hibernate-mapping>
