package dmdd.dmddjava.service.historyservice;

public class AmountHistoryCalContext {
	
	private Long proId;
	private Long orgId;
	private int isSuit;
	private int period;
	private Double oriValue;
	
	private Long bizDataId;
	private Long amountBizDataId;
	
	private Long bizDataIdAd;
	private Long amountBizDataIdAd;
	
	public Long getProId() {
		return proId;
	}
	public void setProId(Long proId) {
		this.proId = proId;
	}
	public Long getOrgId() {
		return orgId;
	}
	public void setOrgId(Long orgId) {
		this.orgId = orgId;
	}
	public int getIsSuit() {
		return isSuit;
	}
	public void setIsSuit(int isSuit) {
		this.isSuit = isSuit;
	}
	public int getPeriod() {
		return period;
	}
	public void setPeriod(int period) {
		this.period = period;
	}
	public Double getOriValue() {
		return oriValue;
	}
	public void setOriValue(Double oriValue) {
		this.oriValue = oriValue;
	}
	public Long getBizDataId() {
		return bizDataId;
	}
	public void setBizDataId(Long bizDataId) {
		this.bizDataId = bizDataId;
	}
	public Long getAmountBizDataId() {
		return amountBizDataId;
	}
	public void setAmountBizDataId(Long amountBizDataId) {
		this.amountBizDataId = amountBizDataId;
	}
	public Long getBizDataIdAd() {
		return bizDataIdAd;
	}
	public void setBizDataIdAd(Long bizDataIdAd) {
		this.bizDataIdAd = bizDataIdAd;
	}
	public Long getAmountBizDataIdAd() {
		return amountBizDataIdAd;
	}
	public void setAmountBizDataIdAd(Long amountBizDataIdAd) {
		this.amountBizDataIdAd = amountBizDataIdAd;
	}

}
