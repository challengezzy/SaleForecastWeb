<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="dmdd.dmddjava.dataaccess.dataobject.Bulletin" 
    table="BULLETIN"
>

    <id
        name="id"
        type="java.lang.Long"
        column="ID"
        unsaved-value="0"
    >
        <meta attribute="use-in-equals">true</meta>
        <generator class="identity" />
    </id>

    <version
        name="version"
        type="long"
        column="VERSION"
    />

    <property
        name="title"
        type="java.lang.String"
        column="TITLE"
        not-null="true"
        unique="true"
        length="255"
    />
    <property
        name="summary"
        type="java.lang.String"
        column="SUMMARY"
        length="255"
    />
    <property
        name="content"
        type="java.lang.String"
        column="CONTENT"
        length="2048"
    />
    <property
        name="createTime"
        type="java.sql.Timestamp"
        column="CREATETIME"
        not-null="true"
        length="10"
    />
    <property
        name="modifyTime"
        type="java.sql.Timestamp"
        column="MODIFYTIME"
        not-null="true"
        length="10"
    />
    <property
        name="publishTime"
        type="java.sql.Timestamp"
        column="PUBLISHTIME"
        length="10"
    />
    <property
        name="isPublish"
        type="int"
        column="ISPUBLISH"
        not-null="true"
        length="2"
    />
    <property
        name="comments"
        type="java.lang.String"
        column="COMMENTS"
        length="255"
    />

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to BulletinOperatorUser -->
    <set
        name="bulletinOperatorUsers"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <key>
            <column name="BULLETINID" />
        </key>
        <one-to-many 
            class="dmdd.dmddjava.dataaccess.dataobject.BulletinOperatorUser"
        />
    </set>
    <!-- uni-directional many-to-one association to OperatorUser -->
    <many-to-one
        name="operatorUser"
        class="dmdd.dmddjava.dataaccess.dataobject.OperatorUser"
    >
        <column name="OPERATORUSERID" />
    </many-to-one>

</class>
</hibernate-mapping>
