<joined-subclass 
    name="dmdd.dmddjava.dataaccess.dataobject.BizDataDefItemFcHand"
    table="BIZDATADEFITEMFCHAND" 
    extends="dmdd.dmddjava.dataaccess.dataobject.BizDataDefItem"
>

    <key column="ID"/>

    <property
        name="coefficient"
        type="java.lang.Double"
        column="COEFFICIENT"
        not-null="true"
        length="18"
    />

    <!-- Associations -->
  
 
    <!-- uni-directional many-to-one association to BizData -->
    <many-to-one
        name="itemBizData"
        class="dmdd.dmddjava.dataaccess.dataobject.BizData"
    >
        <column name="ITEMBIZDATAID" />
    </many-to-one>

</joined-subclass> 
</hibernate-mapping>
