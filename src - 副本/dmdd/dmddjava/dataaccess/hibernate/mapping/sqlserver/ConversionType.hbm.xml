<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="dmdd.dmddjava.dataaccess.dataobject.ConversionType" 
    table="CONVERSIONTYPE"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="id"
        type="java.lang.Long"
        column="ID"
        unsaved-value="0"
    >
        <meta attribute="use-in-equals">true</meta>
        <generator class="identity" />
    </id>

    <version
        name="version"
        type="long"
        column="VERSION"
    />

    <property
        name="code"
        type="java.lang.String"
        column="CODE"
        not-null="true"
        unique="true"
        length="255"
    />
    <property
        name="name"
        type="java.lang.String"
        column="NAME"
        not-null="true"
        unique="true"
        length="255"
    />
    <property
        name="proportion"
        type="long"
        column="PROPORTION"
        length="10"
    />  

     <!-- Associations -->
  
    <!-- bi-directional many-to-one association to UnitGroup -->
    <many-to-one
        name="unitGroup"
        class="dmdd.dmddjava.dataaccess.dataobject.UnitGroup"
        not-null="true"
    >
        <column name="UNITGROUPID" />
    </many-to-one>
</class>
</hibernate-mapping>
