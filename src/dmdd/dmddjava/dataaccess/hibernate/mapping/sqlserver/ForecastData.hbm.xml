<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="dmdd.dmddjava.dataaccess.dataobject.ForecastData" 
    table="FORECASTDATA"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="id"
        type="java.lang.Long"
        column="ID"
        unsaved-value="0"
    >
        <meta attribute="use-in-equals">true</meta>
        <generator class="identity" />
    </id>

    <version
        name="version"
        type="long"
        column="VERSION"
    />

    <property
        name="period"
        type="int"
        column="PERIOD"
        not-null="true"
        length="6"
    />
    <property
        name="value"
        type="long"
        column="VALUE"
        not-null="true"
        length="15"
    />
    <property
        name="status"
        type="int"
        column="STATUS"
        not-null="true"
        length="2"
    />
    <property
        name="initTime"
        type="java.sql.Timestamp"
        column="INITTIME"
        not-null="true"
        length="10"
    />
    <property
        name="updateTime"
        type="java.sql.Timestamp"
        column="UPDATETIME"
        not-null="true"
        length="10"
    />
    <property
        name="comments"
        type="java.lang.String"
        column="COMMENTS"
        length="255"
    />

    <!-- Associations -->
  
    <!-- uni-directional many-to-one association to BizData -->
    <many-to-one
        name="bizData"
        class="dmdd.dmddjava.dataaccess.dataobject.BizData"
    >
        <column name="BIZDATAID" />
    </many-to-one>
    <!-- uni-directional many-to-one association to Organization -->
    <many-to-one
        name="organization"
        class="dmdd.dmddjava.dataaccess.dataobject.Organization"
    >
        <column name="ORGANIZATIONID" />
    </many-to-one>
    <!-- uni-directional many-to-one association to Product -->
    <many-to-one
        name="product"
        class="dmdd.dmddjava.dataaccess.dataobject.Product"
    >
        <column name="PRODUCTID" />
    </many-to-one>

</class>
</hibernate-mapping>
